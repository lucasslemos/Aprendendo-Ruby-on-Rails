<%= @admin.inspect %>

<div id="default">
  <div class="card mb-4">
      <div class="card-header">Editando os Admins</div>
      <div class="card-body">
          <!-- Component Preview-->
          <div class="sbp-preview">
              <div class="sbp-preview-content">
                <%= form_with(model: [ :admins_backoffice, @admin], local: true) do |form| %>
                      <div class="form-group">
                          <%= form.label :email %>
                          <%= form.text_field :email, class:"form-control", placeholder:"name@example.com" %>
                      </div>
                      
                      <div class="form-group">
                        <%= form.label :password %>
                        <%= form.text_field :password, class:"form-control", placeholder:"name@example.com" %>
                      </div>
                      
                      <div class="form-group">
                        <%= form.label :password_confirmation %>
                        <%= form.text_field :password_confirmation, class:"form-control", placeholder:"name@example.com" %>
                      </div>

                  <button type="submit" class="btn btn-primary"> SALVAR </button>
                  <% end %>
                      </div>
              
                  </div>
              </div>
          </div>
      </div>
